{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "20.13.1"
  },
  "scripts": {
    "build": "tsc -p .",
    "start": "node build/src/index.js",
    "dev": "node --env-file=.env --import=tsx --watch ./src/index.ts",
    "lint": "eslint . --fix",
    "test": "jest",
    "docker:start": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "docker:serve": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "dockers:exec:api": "docker exec -it kwamsc-api /bin/sh",
    "dockers:exec:firebase": "docker exec -it firebase-emulator /bin/sh",
    "firebase-export-seed": "firebase emulators:export seed",
    "firebase:emulator-start": "firebase emulators:start --import=seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@firebase/rules-unit-testing": "^3.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
