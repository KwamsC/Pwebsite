    "@eslint/js": "^9.12.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.11",
    "@types/supertest": "6.0.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.8.1"
  },
  "dependencies": {
    "@fastify/cors": "9.0.1",
    "@fastify/helmet": "11.1.1",
